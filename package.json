    "test": "cross-env FOOBAR=FOOBAR ./node_modules/karma/bin/karma start",
    "watch": "run-p watch:webpack start",
    "watch:webpack": "webpack-cli --mode=development --config webpack.config.js --watch --env.watch"
  },
